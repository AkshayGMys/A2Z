class Solution {
public:
    int climbStairs(int n) 
    {
        if(n == 1)
            return 1;
        else if(n==2)
            return 2;
        vector<int> ans(n,1);
        ans[n-2] = 2;
        for(int i=n-3;i>=0;i--)
            ans[i] = ans[i+1] + ans[i+2];
        return ans[0];
    }
};
