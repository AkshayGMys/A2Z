//EASY PEASY

class Solution {
  public:
    int findMin(int n) {
        // code here
        int count = 0;
        while(n>0 && n>=10)
        {
            n-=10;
            count++;
        }
         while(n>0 && n>=5)
        {
            n-=5;
            count++;
        }
         while(n>0 && n>=2)
        {
            n-=2;
            count++;
        }
         while(n>0 && n>=1)
        {
            n-=1;
            count++;
        }
        return count;
    }
};
