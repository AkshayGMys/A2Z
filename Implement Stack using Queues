//I am just implementing the stack here ,Not implementing the queues with stack



class MyStack {
public:
    int arr[1000];
    int topPointer = -1;
    int size = 1000;
    MyStack() 
    {
        
    }
    
    void push(int x) 
    {
        if(topPointer < size)
            arr[++topPointer] = x;
    }
    
    int pop() {
        if(topPointer>=0)
        {
            int a = arr[topPointer--];
            return a;
        } 
        return -1;
    }
    
    int top() {
        if(topPointer>=0)
            return arr[topPointer];
        return -1;
    }
    
    bool empty() {
        if(topPointer == -1)
            return true;
        return false;
    }
};

/**
 * Your MyStack object will be instantiated and called as such:
 * MyStack* obj = new MyStack();
 * obj->push(x);
 * int param_2 = obj->pop();
 * int param_3 = obj->top();
 * bool param_4 = obj->empty();
 */
