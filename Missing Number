CONCEPT : XOR ing give array elements and set of n numbers gives the answer;

class Solution {
public:
    int missingNumber(vector<int>& nums) 
    {
        int res = 0;
        for(auto &it : nums)
            res = res ^ it;
        int actual = 0;
        for(int i=0;i<=nums.size();i++)
        {
            actual = actual ^ i;
        }
        return actual ^ res;
    }
};
